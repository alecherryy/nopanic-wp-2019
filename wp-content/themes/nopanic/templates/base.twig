			<div class="front-intro front-intro--red">
					<h1 class="is-in">No panic</h1>
					<a class="front-intro-link front-intro-link--red" href="#">View more</a>
			</div>
			<div class="front-intro front-intro--white">
				<h1>Do it just</h1>
				<a class="front-intro-link front-intro-link--white" href="#">View more</a>
				</div>
			</div>
		{% endif %} #}
		<header class="header" >
			{% block header %}
				<div class="header-wrapper">
					<h1 class="site-logo" role="banner">
						<a class="site-logo-link" href="{{ site.url }}" rel="home">{{ site.name }}</a>
					</h1>
					<nav id="nav-main" class="nav-main" role="navigation">
						<div class="nav-main-inner">
							{% include "menu.twig" with {'menu': menu.get_items} %}
						</div>
					</nav><!-- #nav -->
				</div>
			{% endblock %}
		</header>

		<section id="content" role="main" class="content-wrapper">
			<div class="wrapper {{ sidebar_class }}">
				{% block content %}
					Sorry, no content
				{% endblock %}
			</div>
			{% if sidebar %}
				<aside class="layout-sidebar">
					{{sidebar}}
				</aside>
			{% endif %}
		</section>

		{% block footer %}
			<footer class="site-footer">
				{% if social_menu.items %}
				<ul class="menu menu--social">
